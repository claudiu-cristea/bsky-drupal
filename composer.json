{
  "type": "project",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "ext-pdo": "*",
    "monolog/monolog": "^3.8",
    "nyholm/psr7": "^1.8",
    "potibm/phluesky": "^0.4.0",
    "symfony/http-client": "^7.2"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "BSkyDrupal\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "environment.php"
    ]
  }
}
